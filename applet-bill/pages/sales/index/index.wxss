.weui-navbar {
  color: #8c8c8c;
  font-size: 30rpx;
  background-color: #fff;
  border-top: 1rpx solid #d7d6dc;
}

picker{
  display: inline-block;
  border-bottom: 1rpx solid #e6e6e6;
  padding: 20rpx 30rpx;
}

.modal-content{
  padding: 30rpx;
  text-align: center;
}

.weui-navbar__item {
  line-height: 98rpx;
  padding: 0rpx;
  height: 98rpx;
}

.cate-wrap {
  background-color: #fff;
  white-space: nowrap;
  padding: 20rpx 30rpx;
  border-top: 4px solid #efeff4;
}

.cate-wrap button {
  font-size: 26rpx;
  border: none;
  background-color: #efeff4;
  border-radius: 6rpx;
  line-height: 60rpx;
  height: 60rpx;
  padding: 0;
  margin-right: 20rpx;
}

.cate-wrap button:last-child {
  margin-right: 0;
}

.cate-wrap button.on {
  background-color: rgba(71, 110, 201, 0.15);
}

.cate-wrap button::after {
  border: none !important;
}

.table-wrap {
  padding: 0 30rpx;
}

.table-wrap .weui-flex {
  align-items: center;
  padding: 0 30rpx;
}

.table-head {
  height: 150rpx;
  color: #fff;
  font-size: 26rpx;
  border-radius: 12rpx 12rpx 0px 0px;
}

.table-head .name {
  font-size: 34rpx;
}

.table .name {
  font-size: 34rpx;
  -webkit-box-flex: 3;
  -webkit-flex: 3;
  flex: 3;
}

.table .aomout {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
  flex: 2;
}

.table-body {
  background-color: #fff;
  position: relative;
  color: #666;
  font-size: 32rpx;
}

.table-body::after {
  content: " ";
  position: absolute;
  left: 30rpx;
  right: 30rpx;
  bottom: 0;
  height: 1px;
  color: #e6e6e6;
  border-bottom: 1rpx solid #e6e6e6;
}

.table-body:last-child:after {
  border-bottom: 1rpx solid transparent;
}

.table-body .weui-flex__item {
  padding: 22rpx 0;
}

.f1 {
  font-size: 40rpx;
}

.table1 .table-head {
  background-image: linear-gradient(110deg, #5480f1 0%, #2d65f4 100%);
}

.table2 .table-head {
  background-image: linear-gradient(110deg, #8593fa 0%, #b588f7 100%);
}

.table3 .table-head {
  background-image: linear-gradient(110deg, #ffaa31 0%, #ff7b02 100%);
}

.table4 .table-head {
  background-image: linear-gradient(110deg, #b960e0 0%, #7327fe 100%);
}

.table5 .table-head {
  background-image: linear-gradient(110deg, #fab77b 0%, #fc76b3 100%);
}
