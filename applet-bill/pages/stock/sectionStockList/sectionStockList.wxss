.list-item {
  background-color: #fff;
  padding:0rpx 30rpx;
  border-top:1rpx solid #d7d6dc;
}

.title {
  position: relative;
  line-height: 50rpx;
  height: 110rpx;
  font-size: 34rpx;
  color: #333;
  padding: 30rpx 0rpx 10rpx 0rpx;

}

.list-item-body {
  font-size: 32rpx;
  padding-bottom: 20rpx;
}


.my-stock {
  color: #476ec9;
  position: absolute;
  right: 0px;
  bottom: 20rpx;
}

.other-stock {
  color: #ffaf23;
}

.weui-navbar{
    background-color:#fff;
    border-bottom:none;
}

.weui-navbar__title{
  font-size: 30rpx;
}
.weui-navbar__slider{
  width: 150px;
  background-color:#476ec9;
}
.weui-navbar__item{
  color:#8c8c8c;
line-height:120rpx;
padding:0rpx;
height:120rpx;
}

.weui-navbar__item.weui-bar__item_on{
  color:#476ec9;
}

.weui-tab__panel {
  padding-top:120rpx
}
