
.cate-wrap {
  padding: 18rpx;
  background-color: #fff;
}

.cate-cotent {
  height: 60rpx;
  white-space: nowrap;
}

.cate-item {
  display: inline-block;
  line-height: 60rpx;
  height: 60rpx;
  font-size: 26rpx;
  color: #000;
  background-color: #efeff4;
  border-radius: 6rpx;
  margin-right: 18rpx;
  padding: 0rpx 50rpx;
}

.cate-item::after {
  border: none !important;
}

.cate-item.on {
  background-color: #476ec9;
  color: #fff;
}

.cate-item:last-child {
  margin-right: 0rpx;
}

.list-item {
  background-color: #fff;
  padding-left: 30rpx;
}

.title {
  line-height: 98rpx;
  height: 98rpx;
  border-bottom: 1rpx solid #E6E6E6;
  font-size: 34rpx;
  color: #333;
}

.list-item-body {
  font-size: 32rpx;
  padding-bottom: 20rpx;
}

.list-item-body .weui-flex {
  margin-top: 20rpx;
}

.my-stock {
  color: #476ec9;
}

.other-stock {
  color: #ffaf23;
}

.lp {
   color: #8C8C8C;
}

.icon-shouji1{
  color: #476ec9;
  font-size: 48rpx;
  position: relative;
  top:6rpx;
}