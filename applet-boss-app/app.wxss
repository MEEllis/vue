}

.cate-wrap {
  padding: 16rpx;
  background-color: #fff;
}

.cate-cotent {
  height: 60rpx;
  white-space: nowrap;
}

.cate-item {
  display: inline-block;
  line-height: 60rpx;
  height: 60rpx;
  font-size: 26rpx;
  color: #000;
  background-color: #efeff4;
  border-radius: 6rpx;
  margin-right: 18rpx;
  padding: 0rpx 30rpx;
}

.cate-item::after {
  border: none !important;
}

.cate-item.on {
  background-color: #21a4ff;
  color: #fff;
}

.cate-item:last-child {
  margin-right: 0rpx;
}

.sum-wrap {
  background-color: #fff;
