  font-size: 40rpx;
  line-height: 1;
  margin-right: 12rpx;
  margin-left: 6rpx;
}

.icon-xiangxia-copy {
  padding-left: 8rpx;
}

.icon-40.left:before {
  transform: rotate(180deg);
  display: inline-block;
  color: #999;
}

.icon-gouxuan {
  color: #10d0b3;
}

.icon-xiangxia-copy:before {
  transform: rotate(0deg);
  display: inline-block;
  transition: transform 0.4s linear;
}

.icon-xiangxia-copy.active:before {
  transform: rotate(-180deg);
  transition: transform 0.4s linear;
}

@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }
}

@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}
