/*清除浮动代码*/

.clearfix:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}

.clearfix {
  zoom: 1;
}

.tl {
  text-align: left;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.container {
  background-color: #efeff4;
}

.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.ellipsis2 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.sel-time {
  position: relative;
  font-size: 22rpx;
  color: #666;
  line-height: 50rpx;
  text-align: center;
  height: 50rpx;
}

.border-top-20 {
  border-top: 20rpx solid #efeff4;
}

.bordotted-top, .bordotted-bottom, .border-bottom, .border-top {
  position: relative;
}

.bordotted-top:before, .bordotted-bottom:before, .border-bottom:before,
.border-top:before {
  content: " ";
  position: absolute;
  right: 0;
  left: 0;
  height: 1rpx;
  color: #d9d9d9;
  z-index: 2;
}

.border-top:before {
  top: 0;
  border-top: 1rpx solid #d9d9d9;
}

.border-bottom:before {
  bottom: 0;
  border-bottom: 1rpx solid #d9d9d9;
}

.bordotted-top:before {
  top: 0;
  border-top: 1rpx dotted #d9d9d9;
}

.bordotted-bottom:before {
  bottom: 0;
  border-bottom: 1rpx dotted #d9d9d9;
}

.btnBlue {
  background-color: rgba(71, 110, 201, 1) !important;
